{
	"hosting": [{
		"target": "dev",
		"public": "dist",
		"ignore": [
			"firebase.json",
			"**/.*",
			"**/node_modules/**"
		],
		"rewrites": [{
			"source": "**",
			"destination": "/index.html"
		}],
		"headers": [{
				"source": "/**",
				"headers": [{
					"key": "Cache-Control",
					"value": "no-cache, no-store"
				}]
			},
			{
				"source": "**/*.@(jpg|jpeg|gif|png|svg|webp|js|css|eot|otf|ttf|ttc|woff|woff2|font.css)",
				"headers": [{
					"key": "Cache-Control",
					"value": "max-age=604800"
				}]
			}
		]
	},
	{
		"target": "production",
		"public": "dist",
		"ignore": [
			"firebase.json",
			"**/.*",
			"**/node_modules/**"
		],
		"rewrites": [{
			"source": "**",
			"destination": "/index.html"
		}],
		"headers": [{
				"source": "/**",
				"headers": [{
					"key": "Cache-Control",
					"value": "no-cache, no-store"
				}]
			},
			{
				"source": "**/*.@(jpg|jpeg|gif|png|svg|webp|js|css|eot|otf|ttf|ttc|woff|woff2|font.css)",
				"headers": [{
					"key": "Cache-Control",
					"value": "max-age=604800"
				}]
			}
		]
	}]
}